{
  "name": "bundle-utils",
  "version": "0.0.2",
  "description": "Determine best bundle for useragent on the flight.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/yavorsky/bundle-utils.git"
  },
  "keywords": [
    "express",
    "webpack",
    "node",
    "babel",
    "babel-preset-env"
  ],
  "author": "Artem Yavorsky",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yavorsky/bundle-utils.git"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/test/jest.config.js",
    "collectCoverageFrom": [
      "./utils/**/*.js",
      "./*.js"
    ],
    "coverageReporters": [
      "text",
      "lcov"
    ]
  },
  "homepage": "https://github.com/yavorsky/bundle-utils",
  "scripts": {
    "start": "npm run build:examples && npm run build && npm run server:start",
    "server:start": "node server",
    "server:dev": "nodemon server",
    "build": "node_modules/.bin/webpack",
    "build:examples": "node ./utils/scripts/generate-examples.js",
    "eslint": "eslint .",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/polyfill": "7.0.0",
    "@babel/register": "7.0.0",
    "babel-loader": "^8.0.4",
    "backbone": "*",
    "browserslist": "^4.2.0",
    "glob": "^7.1.3",
    "md5": "2.2.1",
    "ua-parser-js": "^0.7.18"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.1.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.24.1",
    "express": "^4.16.3",
    "flow-bin": "^0.82.0",
    "flow-typed": "^2.5.1",
    "fs-extra": "^7.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "supertest": "^3.3.0",
    "webpack": "4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3"
  }
}
